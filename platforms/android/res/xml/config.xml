<?xml version='1.0' encoding='utf-8'?>
<widget id="com.avmishra.shoppinglist" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>ShoppingList</name>
    <icon src="icon.png" />
    <description>
        An application to list all daily life shopping.
    </description>
    <author email="avadhesh.mishra@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="login.html" />
    <access origin="http://127.0.0.1*" />
    <access origin="*" />
</widget>
